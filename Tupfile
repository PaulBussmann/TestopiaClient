include_rules

srcs-TestopiaClient = src/TestopiaRpcClient.cpp src/TestopiaClient.cpp
srcs-TestopiaClientTest = src/TestopiaClientTest.cpp

# -std=c++11 -Isrc 
: foreach $(srcs-TestopiaClient) |> !cpp_x86 -IC:/CppUTest/include -IC:/CppUTest/include/Platforms/Gcc -std=c++11 |> build-Linux/x86/Debug/%B.o {objs-TestopiaClient}
: {objs-TestopiaClient} |> !ar_x86 |> build-Linux/x86/Debug/libTestopiaClient.a

: foreach $(srcs-TestopiaClientTest) |> !cpp_x86 -IC:/CppUTest/include -IC:/CppUTest/include/Platforms/Gcc -std=c++11 -DTESTOPIA_CLIENT_TEST |> build-Linux/x86/Debug/%B.o {objs-TestopiaClientTest}
: build-Linux/x86/Debug/libTestopiaClient.a {objs-TestopiaClientTest} |> $(PREFIX_X86)g++ -std=c++11 $(LDFLAGS_X86) -o build-Linux/x86/Debug/TestopiaClientTest build-Linux/x86/Debug/TestopiaClientTest.o -Lbuild-Linux/x86/Debug -ltestopiaclient -lulxmlrpcpp -lcpputest -LC:/CppUTest/lib/Linux/x86 |> build-Linux/x86/Debug/TestopiaClientTest 
